    "array = [19 , 10 , 7 , 9 , 1 , 2 , 50]\n",
    "j = 0\n",
    "for i in range(len(array)):\n",
    "    key = array[i]\n",
    "    for j in range(len(array)-i-1):\n",
    "        if array[j] > array[j+1]:\n",
    "            array[j] = array[j+1]\n",
    "            array[j+1] = key\n",
    "            print(array)"
